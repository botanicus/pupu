# dependencies of the pupu
<% @dependencies.each do |dependency| %>
dependency :<%= dependency %>
<% end %>

# load javascripts
<% @javascripts.each do |javascript| %>
javascript "<%= javascript %>"
<% end %>

# load stylesheets
<% @stylesheets.each do |stylesheet| %>
stylesheet "<%= stylesheet %>"
<% end %>

